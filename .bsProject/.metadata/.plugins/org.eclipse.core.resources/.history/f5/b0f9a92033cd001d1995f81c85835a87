<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/sqldirect" elementFormDefault="qualified" targetNamespace="http://www.example.org/sqldirect">

    <complexType name="column">
    	<sequence>
    		<element maxOccurs="unbounded" minOccurs="0" name="name" type="string"/>
    		<element maxOccurs="unbounded" minOccurs="0" name="value" type="string"/>
    	</sequence>
    </complexType>

    <element name="column" type="tns:column"/>

    <complexType name="unknownResultset">
    	<sequence>
    		<element maxOccurs="unbounded" minOccurs="0" name="column" type="tns:column"/>
    	</sequence>
    </complexType>

    <element name="row" type="tns:unknownResultset"/>
</schema>
